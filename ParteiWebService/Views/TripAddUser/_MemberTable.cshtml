@using ParteiWebService.ViewModel
@using DataAccessLibrary.Models
@model List<Member>
@{
    ViewData["Title"] = "_MemberTable";
}

@if (Model.Count >= 0)
{
    <div class="table-responsive">
        <table class="table table-bordered dataTables_empty" id="dataTableMember" width="100%" cellspacing="0">
            <thead>
                <tr>
                    <th></th>
                    <th>Vorname</th>
                    <th>Nachname</th>
                </tr>
            </thead>
            <tbody>
                @foreach (Member member in Model)
                {
                    <tr id="@member.ID">
                        <th></th>
                        <td>@member.PreName </td>
                        <td>@member.LastName</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
}